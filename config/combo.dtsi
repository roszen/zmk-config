/ {
  combos {
    compatible = "zmk,combos";

    combo_esc {
      timeout-ms = <50>;
      key-positions = <j k>;
      bindings = <&kp ESC>;
    };

    combo_def {
      timeout-ms = <50>;
      key-positions = <n , . />;
      bindings = <&to DEF>;
    };

    combo_gme {
      timeout-ms = <50>;
      key-positions = <z x c v>;
      bindings = <&to GME>;
    };
  };
};
